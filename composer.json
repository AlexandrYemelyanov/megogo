{
    "name": "xnf4o/megogo",
    "description": "Laravel MeGoGo API integration",
    "license": "MIT",
    "type": "library",
    "keywords": ["Laravel", "Megogo"],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "xnf4o\\Megogo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "xnf4o\\Megogo\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "xnf4o\\Megogo\\MegogoServiceProvider"
            ],
            "aliases": {
                "Megogo": "xnf4o\\Megogo\\Facades\\Megogo"
            }
        }
    }
}
